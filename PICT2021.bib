@Article{Saito1981,
  Title                    = {Study of coating flow by the finite element method },
  Author                   = {H. Saito and L.E. Scriven},
  Journal                  = {Journal of Computational Physics },
  Year                     = {1981},
  Number                   = {1},
  Pages                    = {53--76},
  Volume                   = {42},

  Abstract                 = {Silliman's analysis of slot coating is extended to accommodate film flows with highly bent menisci, as in slide and curtain coating, by combining polar and Cartesian coordinate parametrizations of meniscus shape. The nonlinear algebraic equations from the subdomain/Galerkin weighted residual method and finite element basis functions are solved by Newton's method, which is perfected for the free boundary problem. Convergence behavior is examined. Slot-coating results reveal slow-flow zones and show that at high capillary number and low metering rate the downstream meniscus can no longer attach to the slot. Comparison of Coyne and Elrod's approximate solutions with the finite element solutions shows that their assumption of a parabolic velocity profile leads to reasonable approximations of meniscus shapes.},
