@Article{Cruchaga2016,
  Title                    = {Numerical Modeling and Experimental Validation of Free Surface Flow Problems},
  Author                   = {Cruchaga, M.A. and Battaglia, L. and Storti, M.A. and D'El{\'i}a, J.},
  Journal                  = {Archives of Computational Methods in Engineering},
