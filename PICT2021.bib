@Article{Quecedo2005,
  Title                    = {Comparison of two mathematical models for solving the dam break problem using the {FEM} method},
  Author                   = {M. Quecedo and M. Pastor and M.I. Herreros and J.A. Fernández Merodo and Qinfen Zhang},
  Journal                  = {Computer Methods in Applied Mechanics and Engineering },
