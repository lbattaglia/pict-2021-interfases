@Article{Ramaswamy1987,
  Title                    = {Lagrangian finite element analysis applied to viscous free surface fluid flow},
  Author                   = {Ramaswamy, Balasubramaniam and Kawahara, Mutsuto},
  Journal                  = {International Journal for Numerical Methods in Fluids},
  Year                     = {1987},
