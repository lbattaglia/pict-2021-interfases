@Article{Paz2012,
  Title                    = {{{FastMat}: {A C}++ library for multi-index array computations}},
  Author                   = {Rodrigo R. Paz and Mario A. Storti and Lisandro D. Dalcin and Hugo G. Castro and Pablo A. Kler},
  Journal                  = {Advances in Engineering Software },
