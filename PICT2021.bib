@Article{Cerne2002,
  Title                    = {Numerical errors of the volume-of-fluid interface tracking algorithm},
  Author                   = {\u{C}erne, Gregor and Petelin, Stojan and Tiselj, Iztok},
  Journal                  = {International Journal for Numerical Methods in Fluids},
