}

@Article{Wang2012,
  Title                    = {A hybrid level set-volume constraint method for incompressible two-phase flow},
  Author                   = {Y. Wang and S. Simakhina and M. Sussman},
  Journal                  = {Journal of Computational Physics },
  Year                     = {2012},
