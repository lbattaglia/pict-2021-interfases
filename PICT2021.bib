@Article{Jeong2004,
  Title                    = {Finite element analysis of filling stage in die-casting process using marker surface method and adaptive grid refinement technique},
  Author                   = {Jeong, J. H. and Yang, D. Y.},
  Journal                  = {International Journal for Numerical Methods in Fluids},
