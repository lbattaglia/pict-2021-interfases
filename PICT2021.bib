}

@Article{Hernandez2008,
  Title                    = {A new volume of fluid method in three dimensions - {Part I: Multidimensional advection method with face-matched flux polyhedra}},
  Author                   = {Hern\'andez, J. and L\'opez, J. and G\'omez, P. and Zanzi, C. and Faura, F.},
  Journal                  = {International Journal for Numerical Methods in Fluids},
