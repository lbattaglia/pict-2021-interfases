}

@Article{Wan2009,
  Title                    = {A hybrid scheme based on finite element/volume methods for two immiscible fluid flows},
  Author                   = {Wan, Tian and Aliabadi, Shahrouz and Bigler, Christopher},
  Journal                  = {International Journal for Numerical Methods in Fluids},
  Year                     = {2009},
  Number                   = {8},
  Pages                    = {930--944},
  Volume                   = {61},

  Abstract                 = {We have successfully extended our implicit hybrid finite element/volume (FE/FV) solver to flows involving two immiscible fluids. The solver is based on the segregated pressure correction or projection method on staggered unstructured hybrid meshes. An intermediate velocity field is first obtained by solving the momentum equations with the matrix-free implicit cell-centered FV method. The pressure Poisson equation is solved by the node-based Galerkin FE method for an auxiliary variable. The auxiliary variable is used to update the velocity field and the pressure field. The pressure field is carefully updated by taking into account the velocity divergence field. This updating strategy can be rigorously proven to be able to eliminate the unphysical pressure boundary layer and is crucial for the correct temporal convergence rate. Our current staggered-mesh scheme is distinct from other conventional ones in that we store the velocity components at cell centers and the auxiliary variable at vertices. The fluid interface is captured by solving an advection equation for the volume fraction of one of the fluids. The same matrix-free FV method, as the one used for momentum equations, is used to solve the advection equation. We will focus on the interface sharpening strategy to minimize the smearing of the interface over time. We have developed and implemented a global mass conservation algorithm that enforces the conservation of the mass for each fluid. Copyright © 2009 John Wiley & Sons, Ltd.},
  Doi                      = {10.1002/fld.1997},
  File                     = {:BIB/FIXED/VOF/IJNMF_2009_61_p930.pdf:PDF},
  ISSN                     = {1097-0363},
  Keywords                 = {finite element method, finite volume method, two-fluid flows, freesurface flows, incompressible flows, hybrid method},
  Publisher                = {John Wiley \& Sons, Ltd.},
  Url                      = {http://dx.doi.org/10.1002/fld.1997}
}

@Article{Wang2013,
