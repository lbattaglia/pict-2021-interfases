@Article{Tezduyar2006,
  Title                    = {Interface-tracking and interface-capturing techniques for finite element computation of moving boundaries and interfaces},
  Author                   = {Tayfun E. Tezduyar},
  Journal                  = {Computer Methods in Applied Mechanics and Engineering },
  Year                     = {2006},
  Note                     = {<ce:title>Incompressible CFD</ce:title> },
  Number                   = {23--24},
  Pages                    = {2983--3000},
  Volume                   = {195},

  Abstract                 = {We provide an overview of some of the interface-tracking and interface-capturing techniques we developed for finite element computation of flow problems with moving boundaries and interfaces. This category of flow problems includes fluid–particle, fluid–object and fluid–structure interactions; free-surface and two-fluid flows; and flows with moving mechanical components. Both classes of techniques are based on stabilized formulations. The interface-tracking techniques are based on the deforming-spatial-domain/stabilized space–time (DSD/SST) formulation, where the mesh moves to track the interface. The interface-capturing techniques, developed primarily for free-surface and two-fluid interface flows, are formulated typically over non-moving meshes, using an advection equation in addition to the flow equations. The advection equation governs the evolution of an interface function that marks the location of the interface. We also highlight some of the methods we developed to increase the scope and accuracy of these two classes of techniques. },
  Doi                      = {10.1016/j.cma.2004.09.018},
  File                     = {:BIB/INTRO/CMAME_2006_195_p2983.pdf:PDF},
  ISSN                     = {0045-7825},
  Keywords                 = {Moving boundaries and interfaces},
  Url                      = {http://www.sciencedirect.com/science/article/pii/S0045782505002367}
}

@Article{Tezduyar2003IJNMF,
  Title                    = {Computation of moving boundaries and interfaces and stabilization parameters},
  Author                   = {Tezduyar, Tayfun E.},
  Journal                  = {International Journal for Numerical Methods in Fluids},
  Year                     = {2003},
  Number                   = {5},
  Pages                    = {555--575},
  Volume                   = {43},

  Abstract                 = {The interface-tracking and interface-capturing techniques we developed in recent years for computation of flow problems with moving boundaries and interfaces rely on stabilized formulations such as the streamline-upwind/Petrov–Galerkin (SUPG) and pressure-stabilizing/Petrov–Galerkin (PSPG) methods. The interface-tracking techniques are based on the deforming-spatial-domain/stabilized space–time formulation, where the mesh moves to track the interface. The interface-capturing techniques, typically used with non-moving meshes, are based on a stabilized semi-discrete formulation of the Navier–Stokes equations, combined with a stabilized formulation of the advection equation governing the time-evolution of an interface function marking the interface location. We provide an overview of the interface-tracking and interface-capturing techniques, and highlight how we determine the stabilization parameters used in the stabilized formulations. Copyright © 2003 John Wiley & Sons, Ltd.},
  Doi                      = {10.1002/fld.505},
  File                     = {:BIB/INTRO/IJNMF_2003_43_p555.pdf:PDF},
  ISSN                     = {1097-0363},
  Keywords                 = {moving boundaries and interfaces, SUPG and PSPG formulations, stabilization parameters},
  Publisher                = {John Wiley \& Sons, Ltd.}
}

