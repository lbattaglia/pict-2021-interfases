  issn = {0898-1221},
  keywords = {<!-- Tag Not Handled --><keyword id=#k000005#>Finite difference method},
  owner = {lbattaglia},
  timestamp = {2013.04.29},
  url = {http://www.sciencedirect.com/science/article/pii/S089812211300120X}
}

